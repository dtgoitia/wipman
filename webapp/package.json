{
  "name": "wipman",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://davidtorralba.com/wipman",
  "type": "module",
  "dependencies": {
    "browser-http-client": "^3.2.0",
    "nanoid": "^4.0.2",
    "primeicons": "^6.0.1",
    "primereact": "^9.5.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
